<%-include header.ejs %>

    <div class="bodyContainer">
        <ol>
            <% _.each(items, function(product) { %>
            <li>
                <h1><%= product.name %></h1>
                <p><%= product.short_description %></p>
                <p><%= product.price %></p>   
                <a href="<%= %>" class="button">More info</a>
            </li>
            <% }); %>
        </ol>
    </div>

